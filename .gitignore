# ------------------------------------------------------------
# Python bytecode and cache
# ------------------------------------------------------------
__pycache__/
*.py[cod]
*$py.class

# ------------------------------------------------------------
# Virtual environments
# ------------------------------------------------------------
.env
.venv
venv/
ENV/
env/
.python-version

# ------------------------------------------------------------
# Jupyter Notebook checkpoints
# ------------------------------------------------------------
.ipynb_checkpoints/

# ------------------------------------------------------------
# IDE files
# ------------------------------------------------------------
.vscode/
.idea/
*.swp
*.swo

# ------------------------------------------------------------
# Operating System files
# ------------------------------------------------------------
.DS_Store
Thumbs.db
desktop.ini

# ------------------------------------------------------------
# Data folders (ignore heavy raw data)
# ------------------------------------------------------------
data/raw/*
data/interim/*
data/processed/*

# Keep folder structure
!data/raw/.gitkeep
!data/interim/.gitkeep
!data/processed/.gitkeep

# ------------------------------------------------------------
# Outputs (generated results)
# ------------------------------------------------------------
outputs/tables/*
outputs/figures/*

# Keep folder structure
!outputs/tables/.gitkeep
!outputs/figures/.gitkeep

# ------------------------------------------------------------
# Logs
# ------------------------------------------------------------
*.log

# ------------------------------------------------------------
# Temporary files
# ------------------------------------------------------------
*.tmp
*.bak
*.old

# ------------------------------------------------------------
# Coverage / testing
# ------------------------------------------------------------
.coverage
htmlcov/

# ------------------------------------------------------------
# Build artifacts
# ------------------------------------------------------------
build/
dist/
*.egg-info/

# ------------------------------------------------------------
# Pytest cache
# ------------------------------------------------------------
.pytest_cache/

# ------------------------------------------------------------
# Mypy cache
# ------------------------------------------------------------
.mypy_cache/

# ------------------------------------------------------------
# Mac/Linux temporary
# ------------------------------------------------------------
*.out
